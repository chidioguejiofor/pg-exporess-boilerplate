{
  "name": "never4get-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_PATH=. node index.js ||  NODE_PATH=dist node dist/index.js",
    "dev": "cross-env NODE_PATH=src NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "build": "rm -rf dist && tsc && npm run copy-config-to-dist",
    "zip-dist": "zip -r ../Deployment-File.zip . && cd .. ",
    "copy-config-to-dist": "cp package*.json dist",
    "test:one-unit": "cross-env NODE_ENV=test NODE_PATH=src mocha --timeout 7000 --exit",
    "test:unit": "npm run test:one-unit --  'src/**/*.unit.test.ts'",
    "tdd": "npm run test:unit -- -w 'src/**/*.unit.test.ts'",
    "test:one-integration": "cross-env NODE_ENV=test NODE_PATH=src env-cmd -e test mocha  -r ts-node/register -r src/__tests__/hooks.ts --timeout 20000 --exit",
    "test:integration": "npm run test:one-integration -- 'src/**/*.integration.test.ts' ",
    "create-domain": "node scripts/create-domain.js ",
    "fix-lint": "eslint . --fix",
    "check-lint": "eslint . ",
    "clean": "rm -rf Archive.zip dist",
    "build-ebs": "npm run build && npm run copy-config-to-dist &&  cd dist  &&  zip -r ../Archive.zip . && cd .. ",
    "set-ebs-appname": "node scripts/create-deploy-config.js deploy-config/ebs-config.yaml "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "async-redis": "^2.0.0",
    "async-redis-mock": "^2.0.0",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-random-string": "=3.3.1 ",
    "date-and-time": "^2.3.0",
    "express": "^4.17.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "mocha": "^9.1.3",
    "oauth": "^0.9.15",
    "otplib": "^12.0.1",
    "pg": "^8.7.1",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.3.0",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "chai-subset": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
